   "source": [
    "def forward_prop(W1, b1, W2, b2, X):\n",
    "Z1 = W1.dot(X) + b1\n",
    "A1 - ReLU(Z1)       # saída da 1ª camamda\n",
    "Z2 = W2.dot(A1) + b2\n",
    "A2 = softmax(Z2)\n",
    "return Z1, A1, Z2, A2"
   ]
  }
